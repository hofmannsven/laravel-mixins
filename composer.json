{
    "name": "protonemedia/laravel-mixins",
    "description": "Laravel Mixins",
    "keywords": [
        "protonemedia",
        "laravel-mixins"
    ],
    "homepage": "https://github.com/protonemedia/laravel-mixins",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pascal Baljet",
            "email": "pascal@protone.media",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/mail": "^6.0 || ^7.0",
        "illuminate/view": "^6.0 || ^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0 || ^5.0",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "ProtoneMedia\\LaravelMixins\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProtoneMedia\\LaravelMixins\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ProtoneMedia\\LaravelMixins\\MixinsServiceProvider"
            ]
        }
    }
}